<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.liqun.atguigucode">
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".AtGuiGuApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".HomeActivity"
            android:configChanges="orientation|screenSize|keyboardHidden">
        </activity>
        <activity android:name=".okhttp.activity.OkHttpActivity"></activity>
        <activity android:name=".okhttp.activity.NewsActivity"></activity>
        <activity android:name=".json.activity.NativeParseActivity"></activity>
        <activity android:name=".json.activity.GsonActivity"></activity>
        <activity android:name=".json.activity.FastJsonActivity"></activity>
        <activity android:name=".xutils3.XUtils3MainActivity"></activity>
        <activity android:name=".xutils3.annotation.FragmentXUtils3Activity"/>
        <activity android:name=".xutils3.net.XUtils3NetActivity"/>
        <activity android:name=".afinal.AFinalActivity"/>
        <activity android:name=".volley.VolleyActivity"/>
        <activity android:name=".butterknife.ButterKnifeActivity"/>
        <activity android:name=".eventbus.EventBusActivity"/>
        <activity android:name=".eventbus.SendActivity"/>
    </application>

</manifest>